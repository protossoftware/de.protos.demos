	PhysicalSystem ElevatorDemoBoard_PhysSys {
		NodeRef msp430FreeRTOS : Msp430FreeRTOS_NodeClass
	}
	
	NodeClass Msp430FreeRTOS_NodeClass {
		runtime = RuntimeClass1
		priomin = 0
		priomax = 10
		
